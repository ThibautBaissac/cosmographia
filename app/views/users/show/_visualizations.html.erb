<div class="container mt-5">
  <div class="row row-cols-2 row-cols-lg-3 g-4 mb-4">
    <% visualizations.each do |visualization| %>
      <div class="col">
        <div class="card rounded-4">
          <%= link_to(visualization) do %>
            <div class="ratio ratio-1x1 rounded-4">
              <%= image_tag(visualization.image, class: "disable_img", alt: visualization.title) %>
            </div>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
