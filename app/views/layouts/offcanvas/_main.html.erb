<div class="offcanvas offcanvas-end" data-bs-scroll="true" tabindex="-1" id="offcanvaFeedback" aria-labelledby="offcanvaFeedbackLabel">
  <div class="offcanvas-header">
    <h5 class="offcanvas-title" id="offcanvaFeedbackLabel"><%= t("feedback.title") %></h5>
    <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
  </div>
  <div class="offcanvas-body">
    <%= render("feedbacks/new") %>
  </div>
</div>
