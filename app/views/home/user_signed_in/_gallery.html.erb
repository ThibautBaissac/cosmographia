<% columns.each do |column| %>
  <div class="col-lg-4 col-md-12 mb-4 mb-lg-0">
    <% column.each do |map| %>
      <% if map.image.attached? %>
        <div class="w-100 mb-4 text-center">
          <%= image_tag(map.image, class: "w-100 rounded") %>
          <span class="small fw-light"><%= map.user.email %></span>
        </div>
      <% end %>
    <% end %>
  </div>
<% end %>
